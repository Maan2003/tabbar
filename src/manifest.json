{
    "manifest_version": 2,
  
    "name": "Tab bar",
    "description": "Adds Tab Preview and Start Fresh Feature to Edge(Chromium)",
    "version": "1.0",
    "content_security_policy": "script-src 'self' 'unsafe-eval' 'unsafe-inline';img-src chrome://favicon/ data:; object-src 'self'",
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["back.js"],
      "persistent": true
    },
    "permissions": [
      "tabs",
      "\u003Call_urls>", 
      "storage",
      "unlimitedStorage",
      "webNavigation" 
    ],
    "web_accessible_resources": [ "empty.jpg"]
  }